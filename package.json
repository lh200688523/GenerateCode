{
	"name": "GenerateCode",
	"displayName": "",
	"description": "auto Generate code",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.39.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:projects.setConfig",
		"onCommand:projects.create",
		"onCommand:fileExplorer.openFile",
		"onView:fileExplorer"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "projects.setConfig",
				"title": "项目配置"
			},
			{
				"command": "projects.create",
				"title": "创建项目"
			}
		],
		"menus": {
			"view/title": [
				{
					"command": "projects.create",
					"group": "projects",
					"when": "view == fileExplorer"
				},
				{
					"command": "projects.setConfig",
					"group": "projects",
					"when": "view == fileExplorer"
				}
			]
		},
		"viewsContainers": {
			"activitybar": [
				{
					"id": "treeView",
					"title": "",
					"icon": "public/webview/images/project.svg"
				}
			]
		},
		"views": {
			"treeView": [
				{
					"id": "fileExplorer",
					"name": "项目目录",
					"when": ""
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mkdirp": "^0.5.2",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.12.21",
		"@types/rimraf": "^2.0.3",
		"@types/vscode": "^1.39.0",
		"glob": "^7.1.4",
		"mocha": "^10.2.0",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode-test": "^1.2.0"
	},
	"dependencies": {
		"mkdirp": "^0.5.1",
		"readline": "^1.3.0",
		"rimraf": "^2.7.1"
	}
}
